{
  "openapi": "3.0.2",
  "info": {
    "title": "Booklist RESTful API",
    "version": "1",
    "description": "OpenAPI for Booklist RESTful API",
    "termsOfService": "https://github.com/Tyn-Tian",
    "contact": {
      "name": "Christian",
      "email": "kristian2015@gmail.com",
      "url": "https://www.linkedin.com/in/christian72/"
    },
    "license": {
      "name": "APACHE 2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0"
    }
  },
  "servers": [
    {
      "description": "Booklist RESTful API Server",
      "url": "https://{environment}.christian.com/api/v1",
      "variables": {
        "environment": {
          "description": "Server Enviroment",
          "default": "dev",
          "enum": ["dev", "qa", "prod"]
        }
      }
    }
  ],
  "externalDocs": {
    "description": "linkedin Christian",
    "url": "https://www.linkedin.com/in/christian72/"
  },
  "paths": {
    "/booklist": {
      "get": {
        "summary": "Get all booklist",
        "description": "Get all incompleted booklist by default",
        "parameters": [
          {
            "name": "include completed",
            "in": "query",
            "required": false,
            "description": "Is include completed booklist",
            "schema": {
              "type": "boolean",
              "nullable": true,
              "default": false
            }
          },
          {
            "name": "name",
            "in": "query",
            "required": false,
            "description": "filter booklist by name",
            "schema": {
              "type": "string",
              "nullable": true,
              "minLength": 1,
              "maxLength": 100
            },
            "examples": {
              "Atomic Habits": {
                "description": "Example name Atomic Habits",
                "value": "Atomic Habits"
              },
              "Filosofi Teras": {
                "description": "Example name Filosofi Teras",
                "value": "Filosofi Teras"
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success get all booklist",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "priority": {
                        "type": "number",
                        "format": "int32"
                      },
                      "tags": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      }
                    }
                  }
                },
                "examples": {
                  "success": {
                    "description": "Example success get all booklist",
                    "value": [
                      {
                        "id": "1",
                        "name": "Atomic Habits",
                        "priority": 3,
                        "genre": [
                          "Self Development",
                          "Habits"
                        ]
                      },
                      {
                        "id": "2",
                        "name": "Filosofi Teras",
                        "priority": 5,
                        "genre": [
                          "Self Development",
                          "Filosofi"
                        ]
                      }
                    ]
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create new booklist",
        "description": "Create new booklist to database",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "minLength": 1,
                    "required": true
                  },
                  "priority": {
                    "type": "integer",
                    "format": "int32",
                    "default": 1,
                    "required": true
                  },
                  "genre": {
                    "type": "array",
                    "required": false,
                    "minItems": 1,
                    "items": {
                      "type": "string",
                      "minLength": 1
                    }
                  }
                }
              },
              "examples": {
                "Atomic Habits": {
                  "description": "Example create booklist Atomic Habits",
                  "value": {
                    "name": "Atomic Habits",
                    "priority": 3,
                    "genre": [
                      "Pengembangan Diri", "Habits"
                    ]
                  }
                },
                "Filosofi Teras": {
                  "description": "Example create booklist Filosofi Teras",
                  "value": {
                    "name": "Filosofi Teras",
                    "priority": 2,
                    "genre": [
                      "Pengembangan Diri", "Filosofi"
                    ]
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success create booklist",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "priority": {
                        "type": "number",
                        "format": "int32"
                      },
                      "tags": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      }
                    }
                  }
                },
                "examples": {
                  "Atomic Habits": {
                    "description": "Example create booklist Atomic Habits",
                    "value": {
                      "name": "Atomic Habits",
                      "priority": 3,
                      "genre": [
                        "Self Development",
                        "Habits"
                      ]
                    }
                  },
                  "Filosofi Teras": {
                    "description": "Example create booklist Filosofi Teras",
                    "value": {
                      "name": "Filosofi Teras",
                      "priority": 5,
                      "genre": [
                        "Self Development",
                        "Filosofi"
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/booklist/{booklistId}": {
      "put": {
        "summary": "Update existing booklist",
        "description": "Update existing booklist in database",
        "parameters": [
          {
            "name": "booklistId",
            "in": "path",
            "required": true,
            "description": "Booklist id for update",
            "schema": {
              "type": "string",
              "minLength": 1,
              "maxLength": 100
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "minLength": 1,
                    "required": true
                  },
                  "priority": {
                    "type": "integer",
                    "format": "int32",
                    "default": 1,
                    "required": true
                  },
                  "genre": {
                    "type": "array",
                    "required": false,
                    "minItems": 1,
                    "items": {
                      "type": "string",
                      "minLength": 1
                    }
                  }
                }
              },
              "examples": {
                "Atomic Habits": {
                  "description": "Example create booklist Atomic Habits",
                  "value": {
                    "name": "Atomic Habits",
                    "priority": 3,
                    "genre": [
                      "Pengembangan Diri", "Habits"
                    ]
                  }
                },
                "Filosofi Teras": {
                  "description": "Example create booklist Filosofi Teras",
                  "value": {
                    "name": "Filosofi Teras",
                    "priority": 2,
                    "genre": [
                      "Pengembangan Diri", "Filosofi"
                    ]
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success update booklist",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "priority": {
                        "type": "number",
                        "format": "int32"
                      },
                      "tags": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      }
                    }
                  }
                },
                "examples": {
                  "Atomic Habits": {
                    "description": "Example update booklist Atomic Habits",
                    "value": {
                      "name": "Atomic Habits",
                      "priority": 3,
                      "genre": [
                        "Self Development",
                        "Habits"
                      ]
                    }
                  },
                  "Filosofi Teras": {
                    "description": "Example update booklist Filosofi Teras",
                    "value": {
                      "name": "Filosofi Teras",
                      "priority": 5,
                      "genre": [
                        "Self Development",
                        "Filosofi"
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      },
      "delete": {
        "summary": "Delete existing booklist",
        "description": "Delete existing booklist in database",
        "parameters": [
          {
            "name": "booklistId",
            "in": "path",
            "required": true,
            "description": "Booklist id for deteled",
            "schema": {
              "type": "string",
              "minLength": 1,
              "maxLength": 100
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success delete booklist",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    }
                  }
                },
                "examples": {
                  "success": {
                    "description": "Success delete booklist",
                    "value": {
                      "success": true
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Failed delete booklist",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    }
                  }
                },
                "examples": {
                  "success": {
                    "description": "Failed delete booklist",
                    "value": {
                      "success": false
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
